{
     "extends": [
          "airbnb",
          "prettier"
     ],
     "plugins": [
          "prettier"
     ],
     "rules": {
          "prettier/prettier": "error",
          "spaced-comment": "off",
          "no-console": "warn",
          "consistent-return": "off",
          "func-names": "off",
          "object-shorthand": "off",
          "no-return-await": "off",
          "no-underscore-dangle": "off",
          "class-methods-use-this": "off",
          "jsx-indent": 8,
          "prefer-destructuring": [
               "error",
               {
                    "object": true,
                    "array": false
               }
          ],
          "no-unused-vars": [
               "error",
               {
                    "argsIgnorePattern": ""
               }
          ]
     }
}